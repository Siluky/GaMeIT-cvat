{
    "eslint.probe": [
        "javascript",
        "typescript",
        "typescriptreact"
    ],
    "eslint.onIgnoredFiles": "warn",
    "eslint.workingDirectories": [
        {
            "directory": "${cwd}",
        },
        {
            "pattern":  "cvat-*"
        },
        {
            "directory": "tests",
            "!cwd": true
        }
    ],
    "npm.exclude": "**/.env/**",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.pycodestyleEnabled": false,
    "licenser.license": "Custom",
    "licenser.customHeader": "Copyright (C) @YEAR@ Intel Corporation\n\nSPDX-License-Identifier: MIT",
    "files.trimTrailingWhitespace": true,
    "python.pythonPath": ".env/bin/python",
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "name": "cvat",
            "database": "${workspaceFolder:cvat}/db.sqlite3"
        }
    ],
    "favorites.resources": [
        {
            "filePath": "cvat-core/src/api.js",
            "group": "Default"
        },
        {
            "filePath": "cvat-core/src/server-proxy.js",
            "group": "Default"
        },
        {
            "filePath": "cvat-ui/src/components/header/header.tsx",
            "group": "Default"
        },
        {
            "filePath": "cvat-ui/src/cvat-core-wrapper.ts",
            "group": "Default"
        },
        {
            "filePath": "cvat/settings/base.py",
            "group": "Default"
        },
        {
            "filePath": "cvat-ui/src/gamification/components/badges/badge-overview.tsx",
            "group": "Default"
        },
        {
            "filePath": "cvat-ui/src/reducers/root-reducer.ts",
            "group": "Default"
        }
    ],
    "favorites.sortOrder": "MANUAL"
}
